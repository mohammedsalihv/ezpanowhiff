    <style>
        * {
            box-sizing: border-box;
        }

        body {
            margin: 0;
            font-family: 'Segoe UI';
        }

        .nav {
            width: 100%;
            background-color: #ffffff;
            display: flex;
            align-items: center;
            position: relative;
            /* Ensure it's on top */
            z-index: 1000;
        }

        .nav>.nav-header {
            flex: 0.95;
            padding-left: 1rem;
        }

        .nav>.nav-header>.nav-title {
            font-size: 22px;
            color: #000000;
            /* Black text */
            font-weight: 900;
        }

        .nav>.nav-list {
            display: flex;
            gap: 2rem;
        }

        .nav>.nav-list>li {
            list-style-type: none;
        }

        .nav>.nav-list>li a {
            text-decoration: none;
            color: #000000;
            /* Black text */
            cursor: pointer;
            font-weight: 900;
        }

        .nav>#nav-check {
            display: none;
        }

        @media (max-width: 768px) {
            .nav>#nav-check {
                display: block;
                /* Show the menu icon */
            }

            .nav>#nav-check:checked+.nav-list {
                display: block;
                /* Display the menu items when checked */
            }

            /* Change the icon color to black */
            .nav>#nav-check+.nav-list .icon {
                color: #000000;
                /* Black icon color */
                font-weight: 900;
            }
        }

        /* Slider Section Styling */
        .home-slider {
            position: relative;
            margin-bottom: 100px;
        }

        .home-slide-cover {
            background-color: #f0f0f0;
            margin-top: 30px;
            position: relative;
        }

        .hero-slider-1 {
            position: relative;
        }

        .single-hero-slider {
            position: relative;
            background-size: cover;
            /* Ensures image covers container */
            background-position: center;
            width: 100%;
            height: 100vh;
            /* Full viewport height */
            display: flex;
            align-items: center;
        }

        .hero-slider-content-2 {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            text-align: left;
            color: white;
            z-index: 10;
            /* Ensure content is on top */
        }

        .hero-slider-content-2 h4,
        .hero-slider-content-2 h3,
        .hero-slider-content-2 h2,
        .hero-slider-content-2 p,
        .hero-slider-content-2 a {
            animation: fadeInUp 1s ease-in-out;
        }

        .btn-brush-3 {
            background-color: #ff6600;
            border: none;
            color: white;
            padding: 10px 20px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            cursor: pointer;
            transition: 0.3s;
        }

        .btn-brush-3:hover {
            background-color: #ff3300;
        }

        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(20px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        @media (max-width: 480px) {
            .single-hero-slider {
                height: 50vh;
                /* Adjust for mobile */
            }

            .hero-slider-content-2 {
                text-align: center;
                /* Center content on mobile */
            }
        }
    </style>
